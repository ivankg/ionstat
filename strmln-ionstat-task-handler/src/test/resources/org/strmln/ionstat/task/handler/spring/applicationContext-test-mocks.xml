<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:security="http://www.springframework.org/schema/security"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mockito="http://www.mockito.org/spring/mockito"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
              http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security-4.1.xsd
              http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd
              http://www.mockito.org/spring/mockito classpath:spring/mockito.xsd">


	<mockito:mock id="deviceService"
		class="org.strmln.ionstat.service.DeviceService" />

	<mockito:mock id="departmentService"
		class="org.strmln.ionstat.service.DepartmentService" />

	<mockito:mock id="facilityService"
		class="org.strmln.ionstat.service.FacilityService" />

	<mockito:mock id="reportingService"
		class="org.strmln.ionstat.service.ReportingService" />

	<mockito:mock id="roomService" class="org.strmln.ionstat.service.RoomService" />

	<mockito:mock id="sessionService"
		class="org.strmln.ionstat.service.SessionService" />

	<mockito:mock id="sessionTemplateService"
		class="org.strmln.ionstat.service.SessionTemplateService" />

	<mockito:mock id="userProfileService"
		class="org.strmln.ionstat.service.UserProfileService" />
</beans>